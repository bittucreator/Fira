# Fira AI App Environment Variables
# Copy this file to .env and add your actual API keys

# Azure OpenAI GPT-5 Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.cognitiveservices.azure.com/openai/deployments/your-deployment-name

# Standard OpenAI API Key (fallback)
OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration (if not already set)
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key
